<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml" width="400" height="300">
	<mx:Script>
		<![CDATA[
			private function onMapLoaded(event:Event):void{
				width = mapLoader.content.width;
				height = mapLoader.content.height;
			}
		]]>
	</mx:Script>
	<mx:SWFLoader x="0" y="0" id="mapLoader" width="100%" height="100%" scaleContent="false" complete="onMapLoaded(event)"/>
</mx:Canvas>
